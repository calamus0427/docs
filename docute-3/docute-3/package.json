{
  "name": "docute",
  "version": "3.4.8",
  "description": "Effortlessly documentation done right.",
  "license": "MIT",
  "repository": "egoist/docute",
  "author": {
    "name": "EGOIST",
    "email": "0x142857@gmail.com",
    "url": "github.com/egoist"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "npm run build",
    "build": "poi build --config build/vue.config.js && node build/themes",
    "dev": "poi --config build/vue.config.js",
    "dev:docsearch": "poi --config build/vue.config.docsearch.js",
    "build:docsearch": "poi build --config build/vue.config.docsearch.js",
    "dev:disqus": "poi --config build/vue.config.disqus.js",
    "build:disqus": "poi build --config build/vue.config.disqus.js",
    "prepublish": "npm run build",
    "lint": "xo "
  },
  "files": [
    "dist/docute.js",
    "dist/docute.css",
    "dist/docute.js.map",
    "dist/docute.css.map",
    "dist/theme-github.css",
    "dist/theme-github.css.map",
    "plugins"
  ],
  "main": "./dist/docute.js",
  "style": "./dist/docute.css",
  "keywords": [
    "doc"
  ],
  "xo": {
    "extends": [
      "vue-app"
    ],
    "extensions": [
      "js",
      "vue"
    ],
    "envs": [
      "jest",
      "browser"
    ],
    "rules": {
      "import/no-webpack-loader-syntax": 0,
      "import/no-unresolved": 0,
      "import/no-unassigned-import": 0,
      "import/no-extraneous-dependencies": 0,
      "unicorn/filename-case": 0,
      "unicorn/no-abusive-eslint-disable": 0
    },
    "ignores": [
      "docs/config.js"
    ]
  },
  "dependencies": {
    "algoliasearch": "^3.22.1",
    "babel-runtime": "^6.20.0",
    "defined": "^1.0.0",
    "dom-scroll-into-view": "^1.2.1",
    "element-closest": "^2.0.2",
    "eventstop": "^2.1.0",
    "head-to": "^0.1.0",
    "nprogress": "^0.2.0",
    "prismjs": "^1.6.0",
    "throttleit": "^1.0.0",
    "unfetch": "^2.1.2",
    "url-resolve": "^1.0.0",
    "vue-inline": "^1.0.1",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.1.2",
    "web-polyfill": "^0.1.0"
  },
  "devDependencies": {
    "cssbag": "^0.1.0",
    "cssnano": "^3.10.0",
    "eslint-config-vue-app": "^1.3.3",
    "hint.css": "^2.5.0",
    "jest-cli": "^20.0.1",
    "poi": "^8.0.0-rc.10",
    "postcss": "^6.0.1",
    "raw-loader": "^0.5.1",
    "xo": "^0.18.2"
  },
  "jest": {
    "testEnvironment": "node"
  }
}